<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.yommmm.mymybatis.mapper.UserMapper">

    <select id="selectByPrimaryKey" resultType="io.yommmm.mymybatis.entity.User">
        select *
        from user
        where userId = #{userId}
    </select>

    <select id="selectAll" resultType="io.yommmm.mymybatis.entity.User">
        select *
        from user
    </select>

</mapper>

